Here are all the users

<% @users.each do |user| %>
	<br>
	<%= user.email %><br>
	<% user.decks.each do |deck| %>
		<%= deck.title%> 
		<% deck.notes.each do |note| %>
			<%= note.question %>
			<%= note.answer %>	
		<%end%>
	<%end%>
<%end%>
<br>
<%= link_to 'Create a new note.', "/notes/new" %>
